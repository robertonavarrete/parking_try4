<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Lot</th>
      <th>Date parking start</th>
      <th>Date parking end</th>
      <th>Car</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.lot %></td>
        <td><%= event.date_parking_start %></td>
        <td><%= event.date_parking_end %></td>
        <td><%= event.car %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

