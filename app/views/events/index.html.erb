<p id="notice"><%= notice %></p>

<h1>Events</h1>
<div class="container">

  <table>
    <thead>
      <tr>
        <th>Ubicacion</th>
        <th>Date parking start</th>
        <th>Date parking end</th>
        <th>Patente</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    
    <tbody>
      <% @events.each do |event| %>
      <tr>
        <td><%= "#{event.lot.lot_row}""#{event.lot.lot_column}" %></td>
        <td><%= event.date_parking_start %></td>
        <td><%= event.date_parking_end %></td>
        <td><%= event.car.plate %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
  
<div class="container" id="calendar"></div>
<br>

