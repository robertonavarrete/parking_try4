<%= form_with(model: event, local: true) do |form| %>
  <% if event.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(event.errors.count, "error") %> prohibited this event from being saved:</h2>

      <ul>
      <% event.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :lot_id %>
    <%= form.text_field :lot_id %>
  </div>

  <div class="field">
    <%= form.label :date_parking_start %>
    <%= form.date_select :date_parking_start %>
  </div>

  <div class="field">
    <%= form.label :date_parking_end %>
    <%= form.date_select :date_parking_end %>
  </div>

  <div class="field">
    <%= form.label :car_id %>
    <%= form.text_field :car_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
