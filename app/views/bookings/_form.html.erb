<%= form_with(model: booking, local: true) do |form| %>
  <% if booking.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(booking.errors.count, "error") %> prohibited this booking from being saved:</h2>

      <ul>
      <% booking.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :date_parking_start %>
    <%= form.text_field :date_parking_start, id: "init_date_picker" , class: 'datepicker' %>
  </div>

  <div class="field">
    <%= form.label :date_parking_end %>
    <%= form.text_field :date_parking_end, id: "end_date_picker" , class: 'datepicker'%>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
